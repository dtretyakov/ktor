package = winhttp
headers = minwindef.h minwinbase.h winhttp.h
compilerOpts = -DUNICODE
linkerOpts = -lwinhttp

---
static unsigned int GetHResultFromError(unsigned int errorCode) {
    int result;
    if (errorCode < 0) {
        result = errorCode;
    }
    else {
        result = (int)(((unsigned int)errorCode & 0x0000FFFF) | (7 << 16) | 0x80000000);
    }

    return result;
}
